class Car:
    price = 1000000

    def horse_powers(self, horse=100):
        return horse


class Nissan(Car):
    price = 2500000


class Kia(Car):
    price = 1500000


Nissan.horse = 250
print('Машина Nissan цена {} мощность {}'.format(Nissan.price, Nissan.horse))
Kia.horse = 150
print('Машина Kia цена {} мощность {}'.format(Kia.price, Kia.horse))
